<section class="list-data">
    <div class="state">
        <a href="/incubadora/incubacion/view/{{data.ids.id_pedido}}">
            <i class="icon-arrow-left2 positionIcon"></i>
        </a>INCIDENCIAS
    </div>
    {{#if message }}
        <div class="messageError">{{ message }}</div>
    {{/if}}

    {{#if success }}
        <div class="messageSuccess">{{ success }}</div>
    {{/if}}
    <div class="list-cuerpo">
        <div class="addBoton">
            <p class="boton">
                <a href="/incubadora/incidencia/add/{{data.ids.id_incubacion}}&{{data.ids.id_pedido}}">
                    <strong> <i class="icon-plus"></i> Incidencia</strong>
                </a>
            </p>
        </div>
        <div class="data-table-pagination">
            <table id="example1" class="data-table table-hover table-bordered">
                <thead>
                    <tr>
                        <th><p class="description-name">Incidencia</p></th>
                        <th><p class="description">Detalle Incidencia</p></th>
                        <th><p class="description">Cantidad Nacidos</p></th>
                        <th><p class="description">Cantidad Defectuosos</p></th>
                        <th><p class="description">Fecha Registrada</p></th>
                        <th><p class="description">Editar </p></th>
                    </tr>
                </thead>
                <tbody>
                    {{#each data.dataIncidencia}}
                    <tr>
                        <td><p class="description-valor">{{id_incidencias}}</p></td>
                        <td><p class="description">{{descripcion}}</p></td>
                        <td><p class="description">{{cantidad_nacidos}}</p></td>
                        <td><p class="description">{{cantidad_defectuosos}}</p></td>
                        <td><p class="description">{{fecha_registrada}}</p></td>
                        <td>
                            <p class="description"><a href="/incubadora/incidencia/edit/{{id_incidencias}}&{{id_incubacion}}&{{id_pedido}}"><img
                            src="/img/editar.png" alt="icon de Editar"></a></p>
                        </td>
                    </tr>
                    {{/each}}
                </tbody>
            </table>
        </div>
    </div>
</section>